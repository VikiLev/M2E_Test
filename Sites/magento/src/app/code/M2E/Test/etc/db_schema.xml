<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table comment="M2E Test" engine="innodb" name="m2e_test_table" resource="default">
        <constraint referenceId="PRIMARY" xsi:type="primary">
            <column name="id"/>
        </constraint>
        <column length="255" name="id" nullable="false" xsi:type="varchar"/>
        <column length="255" name="purchase_date" nullable="true" xsi:type="varchar"/>
        <column length="255" name="ship_to_name" nullable="true" xsi:type="varchar"/>
        <column length="255" name="customer_email" nullable="true" xsi:type="varchar"/>
        <column name="grant_total" scale="4" precision="20" unsigned="true" nullable="true" xsi:type="decimal"/>
        <column length="255" name="status" nullable="true" xsi:type="varchar"/>
    </table>
</schema>
